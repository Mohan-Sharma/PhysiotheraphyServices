								<attribute name="onCreate">
								self.setChecked(patientSocialHistory.getOtherStress()!= null);
								if(self.isChecked())
									otherStressTxtBox.setVisible(true);
								</attribute>
							</checkbox>
							</hlayout>
							<textbox value="@{patientSocialHistory.otherStress}" id="otherStressTxtBox" visible="false"/>	
								<label value="ChildHood" zclass="styled" />
								<radiogroup orient="horizontal">
									<radio label="Mostly Happy" value="Mostly Happy"
										onCheck="patientSocialHistory.setChildHood(self.getValue())"
										onCreate="self.setSelected(self.getValue().equals(patientSocialHistory.getChildHood()))">
									</radio>
									<radio label="Mostly Painful" value="Mostly Painful"
										onCheck="patientSocialHistory.setChildHood(self.getValue())"
										onCreate="self.setSelected(self.getValue().equals(patientSocialHistory.getChildHood()))">
									</radio>
									<radio label="Normal" value="Normal"
										onCheck="patientSocialHistory.setChildHood(self.getValue())"
										onCreate="self.setSelected(self.getValue().equals(patientSocialHistory.getChildHood()))">
									</radio>
									<radio label="Dont Recall" value="DontRecall"
										onCheck="patientSocialHistory.setChildHood(self.getValue())"
										onCreate="self.setSelected(self.getValue().equals(patientSocialHistory.getChildHood()))">
									</radio>
								</radiogroup>
								<vlayout style="margin:10px">
								<label value="Comments" zclass="styled"/>
								<textbox rows="3" value="@{patientSocialHistory.comments}" cols="90"/>
								</vlayout>
								</vlayout>	
								</groupbox>
					</div>		
					</div>
					<div sclass="buttonPanel">
						<button disabled="${controller.readonly}" label="Save" id="saveBtn" zclass="btn btn-success">
							<attribute name="onClick">
							controller.saveSoapSection();
							Events.postEvent("onClick",(Component)desktop.getAttribute("wkModule"),null);
							Events.postEvent("onSaveClick",addEditSocialWindow.getParent(),null);
							addEditSocialWindow.detach();
							</attribute>
						</button>
					</div>
				</panelchildren>
			</panel>
	</window>
</zk>