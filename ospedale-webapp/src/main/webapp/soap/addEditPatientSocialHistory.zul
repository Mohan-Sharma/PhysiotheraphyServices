<?page title="new page title" contentType="text/html;charset=UTF-8"?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" root="./addEditSocialWindow"?>
<zk xmlns:h="http://www.w3.org/1999/xhtml" xmlns:a="http://www.zkoss.org/2005/zk/annotation"
	xmlns:n="http://www.zkoss.org/2005/zk/native" xmlns:w="http://www.zkoss.org/2005/zk/client">
	<?taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c"?>
	<zscript><![CDATA[
	import com.nzion.enums.SmokingStatusEnum;
	com.nzion.service.SoapNoteService soapNoteService = com.nzion.util.Infrastructure.getSpringBean("soapNoteService");
	com.nzion.zkoss.composer.PatientSoapNoteController controller = arg.get("controller");
	com.nzion.domain.emr.soap.SocialHistorySection socialHistorySection = arg.get("socialHistorySection");
	com.nzion.domain.emr.soap.PatientSocialHistory patientSocialHistory = socialHistorySection.getPatientSocialHistory();
	com.nzion.domain.emr.soap.PatientSocialHistory preceedingSocialHistory = arg.get("preceedingSocialHistory");
	if(patientSocialHistory == null){
		patientSocialHistory = preceedingSocialHistory != null ? preceedingSocialHistory.createCopy() : new com.nzion.domain.emr.soap.PatientSocialHistory();   
		patientSocialHistory.setSoapSection(socialHistorySection);
		patientSocialHistory.setPatient(controller.getPatient());
	}
	socialHistorySection.setPatientSocialHistory(patientSocialHistory);
	boolean isFemale = "Female".equalsIgnoreCase(controller.getPatient().getGender() == null ? null : controller.getPatient().getGender().getDescription());
	List smokingStatuses = Arrays.asList(SmokingStatusEnum.values());
	void setDivVisible(Radio radio, Div div, Boolean b) {
		if (b == null)
			return;
		radio.setSelected(b);
		div.setVisible(b);
	}
	
	void setChecked(String value,Textbox textbox,com.nzion.domain.emr.soap.PatientSocialHistory history){
	history.setBirthControlMethod(value);
	textbox.setVisible(false);
	history.setOtherMethodOfBirthControl(null);	
	}
	
	void setPatientLivingArrangement(com.nzion.domain.emr.soap.PatientSocialHistory patientSocialHistory,Textbox txtBox,String value){
	patientSocialHistory.setLivingArrangement(value);
	txtBox.setVisible(false);
	patientSocialHistory.setOtherLivingArrangement(null);
	}
	
	void setSmokingDivVisible() {
		if (patientSocialHistory.getSmokingStatus() != null
				&& !(SmokingStatusEnum.NEVER_SMOKER.equals(
						patientSocialHistory.getSmokingStatus()) || SmokingStatusEnum.UNKNOWN
						.equals(patientSocialHistory.getSmokingStatus()))) {
			((Div) addEditSocialWindow.getFellow("cigrattesDiv")).setVisible(true);
		} else {
			((Div) addEditSocialWindow.getFellow("cigrattesDiv")).setVisible(false);
		}
	}
]]></zscript>
	<window id="addEditSocialWindow" height="90%" width="90%" mode="modal" closable="true" >
	
			<panel title="Social History" collapsible="true" border="none">
				<panelchildren style="background-color:#F9F9F9;border:0px;">
					<div style="margin:10px">
						<div align="center" style="margin:10px">
						<button label="Save" zclass="btn btn-success" onClick='Events.postEvent("onClick",saveBtn,null)'/>
						<button label="Cancel" onClick='addEditSocialWindow.detach();'/>
						</div>
					<hlayout spacing="5%">
						<hlayout spacing="40px">
							<label value="Terminally Ill" zclass="styled" />
							<checkbox checked="@{patientSocialHistory.terminallyIll}"/>
						</hlayout>
						<hlayout>
							<label value="Occupation" zclass="styled" />
							<enumeration  enumType="OCCUPATION" selectedItem="@{patientSocialHistory.occupation}"/>
						</hlayout>
						<hlayout>
							<label value="Marital Status" zclass="styled" />
							<enumeration  enumType="MARITAL_STATUS" 
								selectedItem="@{patientSocialHistory.maritalStatus}" />
						</hlayout>
						<hlayout>
							<label value="Hobbies" zclass="styled" />
							<textbox value="@{patientSocialHistory.hobbies}" width="90%"/>
						</hlayout>
					</hlayout>
					<div style="margin-top:10px">
						<groupbox closable="false">
							<caption label="Exercise"/>
								<vlayout>
								<hlayout style="margin:10px">
										<label value="Do you Exercise ? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setExercise(true)"
												onCreate="setDivVisible(self,exerciseDiv,patientSocialHistory.getExercise())" value="Yes">
												<attribute name="onClick">
													exerciseDiv.setVisible(true);
													exerciseTxtBox.setVisible(false);
												</attribute>
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setExercise(false);exerciseTxtBox.setVisible(true);"
												onCreate="self.setSelected(!patientSocialHistory.getExercise());exerciseTxtBox.setVisible(!patientSocialHistory.getExercise())" value="No">
												<attribute name="onClick">
													exerciseDiv.setVisible(false);
												</attribute>
											</radio>
										</radiogroup>
										<textbox value="@{patientSocialHistory.reasonForNotExercising}" visible="false" id="exerciseTxtBox"/>
										</hlayout>
										<div sclass="smokingDiv" visible="false" id="exerciseDiv" width="60%" style="margin-left:10px">
											<div>
											<label value="If yes please provide the following information"/>
											<n:table border="0">
												<n:thead>
													<n:tr>
														<n:th>Physical Activity</n:th>
														<n:th>Duration(Minutes)</n:th>
														<n:th>Frequency</n:th>
													</n:tr>
												</n:thead>
												<n:tbody>
													<n:tr>
														<n:td align="center">
															<combobox selectedItem="@{patientSocialHistory.physicalActivityOne}">
																<comboitem label="Racquetball" value="Racquetball"/>
																<comboitem label="Basketball" value="Basketball"/>
																<comboitem label="Baseball" value="Baseball"/>
																<comboitem label="Cycling" value="Cycling"/>
																<comboitem label="Swimming" value="Swimming"/>
															</combobox>
														</n:td>
														<n:td align="center">
															<intbox value="@{patientSocialHistory.activityOneDuration}"/>
														</n:td>
														<n:td align="center">
															<combobox selectedItem="@{patientSocialHistory.activityOneFrequency}">
																<comboitem label="Daily" value="Daily"/>
																<comboitem label="Weekly" value="Weekly"/>
															</combobox>
														</n:td>
													</n:tr> 
													<n:tr>
														<n:td align="center">
															<combobox selectedItem="@{patientSocialHistory.physicalActivityTwo}">
																<comboitem label="Racquetball" value="Racquetball"/>
																<comboitem label="Basketball" value="Basketball"/>
																<comboitem label="Baseball" value="Baseball"/>
																<comboitem label="Cycling" value="Cycling"/>
																<comboitem label="Swimming" value="Swimming"/>
															</combobox>
														</n:td>
														<n:td align="center">
															<intbox value="@{patientSocialHistory.activityTwoDuration}"/>
														</n:td>
														<n:td align="center">
															<combobox selectedItem="@{patientSocialHistory.activityTwoFrequency}">
																<comboitem label="Daily" value="Daily"/>
																<comboitem label="Weekly" value="Weekly"/>
															</combobox>
														</n:td>
													</n:tr> 
													<n:tr>
														<n:td align="center" style="padding-left:20px;">
															<textbox value="@{patientSocialHistory.otherPhysicalActivity}"/>(Other)
														</n:td>
														<n:td align="center">
															<intbox value="@{patientSocialHistory.otherActivityDuration}"/>
														</n:td>
														<n:td align="center">
															<combobox selectedItem="@{patientSocialHistory.otherActivityFrequency}">
																<comboitem label="Daily" value="Daily"/>
																<comboitem label="Weekely" value="Weekely"/>
															</combobox>
														</n:td>
													</n:tr> 
												</n:tbody>
											</n:table>
											</div>
										</div>
										<vlayout style="margin:10px">
											<label value="Exercise Comments" zclass="styled"/>
											<textbox value="@{patientSocialHistory.exerciseComments}" rows="3" cols="90"/>
										</vlayout>
									</vlayout>	
								</groupbox>
					</div>		
					<div style="margin-top:10px">
						<groupbox closable="false">
							<caption label="Diet"/>
								<vlayout>
								<hlayout style="margin:10px">
										<label value="Are you satisfied with your weight ? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setNormalWeight(true)" 
											onCreate="self.setSelected(patientSocialHistory.getNormalWeight());" value="Yes">
												<attribute name="onClick">
													dietTxtBox.setVisible(false);
												</attribute>
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setNormalWeight(false);dietTxtBox.setVisible(true);"
												onCreate="self.setSelected(!patientSocialHistory.getNormalWeight());dietTxtBox.setVisible(!patientSocialHistory.getNormalWeight())" value="No">
											</radio>
										</radiogroup>
										<textbox value="@{patientSocialHistory.abnormalWeightReason}" visible="false" id="dietTxtBox"/>
										</hlayout>
										<div sclass="smokingDiv"  width="70%" style="margin-left:10px">
											<div>
											<label value="Diet(Please select all that apply)"/>
											<hlayout>
											<checkbox label="Carbohydrate Counting" value="Carbohydrate Counting" 	
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
									
											<checkbox label="Low Fat/Low Cholesterol" value="Low Fat/Low Cholesterol"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											
											<checkbox label="Nutritional Shakes" value="Nutritional Shakes"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="Salt" value="Salt"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="High Calorie/Weight gain" value="High Calorie/Weight gain"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="Low Salt" value="Low Salt"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											</hlayout>
											<hlayout>
											<checkbox label="Regular Food" value="Regular Food"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="Vegetarian" value="Vegetarian"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="Low Calorie/Weight loss" value="Low Calorie/Weight loss"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="Low/No Lactose" value="Low/No Lactose"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											<checkbox label="Vegan" value="Vegan"
											onCreate="self.setChecked(patientSocialHistory.getPatientDiet().contains(self.getValue()))"
											onCheck='patientSocialHistory.addOrRemoveDiet(self.getValue(), event.isChecked())'/>
											</hlayout>
											</div>
										</div>
										<vlayout style="margin:10px">
											<label value="Diet Comments" zclass="styled"/>
											<textbox value="@{patientSocialHistory.dietComments}" rows="3" cols="90"/>
										</vlayout>
									</vlayout>	
								</groupbox>
					</div>		
					
					<div style="margin-top:10px">
						<groupbox closable="false">
							<caption label="Tobacco,Alcohol and Drug Use"/>
								<hlayout style="margin:10px">
								<label value="Do you Smoke ? " zclass="styled" />
								<div>
								<combobox onSelect="setSmokingDivVisible()" id="comboboxSmoke"
									selectedItem="@{patientSocialHistory.smokingStatus}"  model="@{smokingStatuses}">
									<comboitem self="@{each='smokingStatus'}" value="@{smokingStatus}"
										label="@{smokingStatus.description}">
									</comboitem>
								</combobox>
								</div>
								<div style="padding:2px" sclass="smokingDiv" 
									id="cigrattesDiv" width="100%" visible="false">
									<hbox width="100%" onCreate="setSmokingDivVisible()">
										<hbox style="padding:8px">
											<label value="Amount of Cigrattes per Day?" />
											<intbox id="amountOfCigaretteIntbox" value="@{patientSocialHistory.cigrattesPerDay}"/>
										</hbox>
										<hbox style="padding:8px">
											<label value="Duration of Cigratte Smoking?" />
											<decimalbox value="@{patientSocialHistory.durationOfSmoking}" />
										</hbox>
										<hbox style="padding:8px">
											<label value="Stopped Smoking Since?"></label>
											<div>
												<decimalbox
													value="@{patientSocialHistory.stoppedSmokingSince}" />
												<listbox mold="select"
													selectedItem="@{patientSocialHistory.smokingTimeUnit}">
													<listitem value="year" label="Year(s)"></listitem>
													<listitem value="month" label="Month(s)"></listitem>
													<listitem value="week" label="Week(s)"></listitem>
												</listbox>
											</div>
										</hbox>
									</hbox>
								</div>
								</hlayout>
								<hlayout style="margin:10px">
										<label value="Do you take Alcohol ? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setAlcohol(true)"
												onCreate="setDivVisible(self,alcoholDiv,patientSocialHistory.getAlcohol())" value="Yes">
												<attribute name="onClick">
													alcoholDiv.setVisible(true);
												</attribute>
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setAlcohol(false)"
												onCreate="self.setSelected(!patientSocialHistory.getAlcohol())" value="No">
												<attribute name="onClick">
													alcoholDiv.setVisible(false);
												</attribute>
											</radio>
										</radiogroup>
										<div style="padding:8px" sclass="smokingDiv" visible="false" id="alcoholDiv">
											<div style="padding:8px">
												<radiogroup>
													<radio label="Moderate Amounts" value="Moderate Amounts"
														onCheck="patientSocialHistory.setAmountOfAlcohol(self.getValue())"
														onCreate="self.setSelected(self.getValue().equals(patientSocialHistory.getAmountOfAlcohol()))">
													</radio>
													<radio label="Heavy Amounts of Alcohol" value="Heavy Amounts"
														onCheck="patientSocialHistory.setAmountOfAlcohol(self.getValue())"
														onCreate="self.setSelected(self.getValue().equals(patientSocialHistory.getAmountOfAlcohol()))">
													</radio>
												</radiogroup>
											</div>
											<div style="padding:8px">
												<label value="Amounts of Alcohol per Week?"></label>
												<textbox value="@{patientSocialHistory.amountOfAlcoholPerWeek}" />
											</div>
										</div>
										</hlayout>
										<hlayout style="margin:10px">
										<label value="Has history of Alcohol ? " zclass="styled" />
										<radiogroup>
											<radio label="Yes"
												onCheck="patientSocialHistory.setAlcoholicHistory(true)"
												onCreate="setDivVisible(self,alcoholicHistoryDiv,patientSocialHistory.getAlcoholicHistory());"
												value="Yes">
												<attribute name="onClick">
													alcoholicHistoryDiv.setVisible(true);	
									           </attribute>
											</radio>
											<radio label="No"
												onCheck="patientSocialHistory.setAlcoholicHistory(false)"
												onCreate="self.setSelected(!patientSocialHistory.getAlcoholicHistory());" value="No">
												<attribute name="onClick">
													alcoholicHistoryDiv.setVisible(false);	
												</attribute>
											</radio>
										</radiogroup>
										<div style="padding:8px" sclass="smokingDiv" visible="false"
											id="alcoholicHistoryDiv">
											<div style="padding:8px">
												<label value="Stopped taking Alcohol Since?"></label>
												<div>
													<intbox value="@{patientSocialHistory.stoppedAlcoholSince}" />
													<listbox mold="select"
														selectedItem="@{patientSocialHistory.alcoholTimeUnit}">
														<listitem value="year" label="Year(s)"></listitem>
														<listitem value="month" label="Month(s)"></listitem>
														<listitem value="week" label="Week(s)"></listitem>
													</listbox>
												</div>
											</div>
										</div>
										</hlayout>
										<hlayout style="margin:10px">
										<label value="Have you ever taken drugs ? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setDrugIntake(true)" value="Yes"
											onCreate="self.setSelected(patientSocialHistory.getDrugIntake());">
											</radio>
											<radio label="No"
												onCheck="patientSocialHistory.setDrugIntake(false)"
												onCreate="self.setSelected(!patientSocialHistory.getDrugIntake());" value="No">
											</radio>
										</radiogroup>
										</hlayout>
										<hlayout style="margin:10px">
											<label value="Have you ever used needle to inject drugs?" zclass="styled"/>										
											<radiogroup>
												<radio label="Yes" onCheck="patientSocialHistory.setNeedleToInjectDrugs(true)" 
												onCreate="self.setSelected(patientSocialHistory.getNeedleToInjectDrugs());" value="Yes">
												</radio>
												<radio label="No"
													onCheck="patientSocialHistory.setNeedleToInjectDrugs(false)"
													onCreate="self.setSelected(!patientSocialHistory.getNeedleToInjectDrugs());" value="No">
												</radio>
											</radiogroup>
										</hlayout>
										<vlayout style="margin:10px">	
											<label value="Drug Comments" zclass="styled"/>
											<textbox value="@{patientSocialHistory.drugComments}" rows="3" cols="90"/>
										</vlayout>
								</groupbox>
					</div>	
						<div style="margin-top:10px">
						<groupbox closable="false">
							<caption label="Safety Issues"/>
								<vlayout>
									<hlayout style="margin:10px">
										<label value=" Do you use a bike helmet? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setHelmets(true)"
												onCreate="self.setSelected(patientSocialHistory.getHelmets()!= null &amp;&amp; patientSocialHistory.getHelmets())" value="Yes">
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setSeatBelts(false);"
												onCreate="self.setSelected(patientSocialHistory.getHelmets()!= null &amp;&amp; !patientSocialHistory.getHelmets());" value="No">
											</radio>
											<radio label="Not Applicable" onCheck="patientSocialHistory.setHelmets(null);"
												onCreate="self.setSelected(patientSocialHistory.getHelmets() == null);">
											</radio>
										</radiogroup>
									</hlayout>
									<hlayout style="margin:10px">
										<label value="Do you use seat belts consistently? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setSeatBelts(true)"
												onCreate="self.setSelected(patientSocialHistory.getSeatBelts()!= null &amp;&amp; patientSocialHistory.getSeatBelts())" value="Yes">
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setSeatBelts(false);"
												onCreate="self.setSelected(patientSocialHistory.getSeatBelts()!= null &amp;&amp; !patientSocialHistory.getSeatBelts());" value="No">
											</radio>
											<radio label="Not Applicable" onCheck="patientSocialHistory.setSeatBelts(null);"
												onCreate="self.setSelected(patientSocialHistory.getSeatBelts() == null);">
											</radio>
										</radiogroup>
									</hlayout>
									<hlayout style="margin:10px">
										<label value="Is violence at home is a concern for you? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setViolenceAtHome(true)"
												onCreate="self.setSelected(patientSocialHistory.getViolenceAtHome()!= null &amp;&amp; patientSocialHistory.getViolenceAtHome())" value="Yes">
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setViolenceAtHome(false);"
												onCreate="self.setSelected(patientSocialHistory.getViolenceAtHome()!= null &amp;&amp; !patientSocialHistory.getViolenceAtHome());" value="No">
											</radio>
											<radio label="Not Applicable" onCheck="patientSocialHistory.setViolenceAtHome(null);"
												onCreate="self.setSelected(patientSocialHistory.getViolenceAtHome() == null);">
											</radio>
										</radiogroup>
									</hlayout>
									<hlayout style="margin:10px">
										<label value="Do you have a gun at home? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setGunAtHome(true)"
												onCreate="self.setSelected(patientSocialHistory.getGunAtHome()!= null &amp;&amp; patientSocialHistory.getGunAtHome())" value="Yes">
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setGunAtHome(false);"
												onCreate="self.setSelected(patientSocialHistory.getGunAtHome()!= null &amp;&amp; !patientSocialHistory.getGunAtHome());" value="No">
											</radio>
											<radio label="Not Applicable" onCheck="patientSocialHistory.setGunAtHome(null);"
												onCreate="self.setSelected(patientSocialHistory.getGunAtHome() == null);">
											</radio>
										</radiogroup>
									</hlayout>
									<hlayout style="margin:10px">
										<label value="Are you afraid of anyone close to you? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setFear(true)"
												onCreate="self.setSelected(patientSocialHistory.getFear()!= null &amp;&amp; patientSocialHistory.getFear())" value="Yes">
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setFear(false);"
												onCreate="self.setSelected(patientSocialHistory.getFear()!= null &amp;&amp; !patientSocialHistory.getFear());" value="No">
											</radio>
											<radio label="Not Applicable" onCheck="patientSocialHistory.setFear(null);"
												onCreate="self.setSelected(patientSocialHistory.getFear() == null);">
											</radio>
										</radiogroup>
									</hlayout>
									<hlayout style="margin:10px">
										<label value="Have you ever been hit,slapped,kicked,pushed,shoved or otherwise physcially hurt
										by your partner or someone close to you? " zclass="styled" />
										<radiogroup>
											<radio label="Yes" onCheck="patientSocialHistory.setBeingHit(true)"
												onCreate="self.setSelected(patientSocialHistory.getBeingHit()!= null &amp;&amp; patientSocialHistory.getBeingHit())" value="Yes">
											</radio>
											<radio label="No" onCheck="patientSocialHistory.setSeatBelts(false);"
